# OpenAI API 配置 (使用 PPIO LLM API)
OPENAI_API_KEY=your_ppio_api_key_here # 从 https://ppio.cloud/settings/key-management 获取
OPENAI_BASE_URL=https://api.ppinfra.com/v3/openai

# 模型配置（可选，默认使用 deepseek/deepseek-v3.1-terminus）
MODEL_NAME=deepseek/deepseek-v3.1-terminus

# AutoGen 配置（可选）
AUTOGEN_USE_DOCKER=false  # 是否使用 Docker 执行代码
MAX_CONSECUTIVE_AUTO_REPLY=10  # 最大自动回复次数

# PPIO API 配置（用于部署到 PPIO Agent Runtime）
PPIO_API_KEY=your_ppio_api_key_here # 从 https://ppio.cloud/settings/key-management 获取
PPIO_AGENT_ID=your_agent_id_here
